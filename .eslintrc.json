{
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-unreachable": "error",
        "no-unused-vars": "error",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}